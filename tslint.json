    "rules": {
        "class-name": true,
        "comment-format": [
            true,
            "check-space"
        ],
        "indent": [
            true,
            "spaces"
        ],
        "no-eval": true,
        "no-internal-module": true,
        "no-trailing-whitespace": true,
        "no-unsafe-finally": true,
        "no-var-keyword": true,
        "one-line": [
            true,
            "check-open-brace",
            "check-whitespace"
        ],
        "quotemark": [
            true,
            "double"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            }
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type",
            "check-module"
        ],
        "max-line-length": [
            true,
            80
        ],
        "interface-name": [
            true,
            "always-prefix"
        ],
        "jsdoc-format": true,
        "member-access": [
            true
        ],
        "member-ordering": [
            true, {
                "order": "fields-first"
            }
        ],
        "prefer-for-of": true,
        "curly": true,
        "no-unused-expression": true,
        "no-unused-variable": [
            true,
            "react"
        ],
        "use-isnan": true,
        "eofline": true,
        "new-parens": true,
        "no-consecutive-blank-lines": [
            true
        ],
        "no-parameter-properties": true,
        "one-variable-per-declaration": [
            true
        ]
    }
}